      "19' metadata={'producer': 'Mac OS X 10.12 Quartz PDFContext', 'creator': 'Microsoft® PowerPoint® 2013', 'creationdate': \"D:20170306114530Z00'00'\", 'moddate': '2018-11-17T11:13:37-06:00', 'title': 'Slide 1', 'author': 'aniketpanda', 'source': '/Users/reejungkim/Documents/Study/Case study/Duke 2017.pdf', 'total_pages': 384, 'page': 18, 'page_label': '19'}\n",
      "{'producer': 'Mac OS X 10.12 Quartz PDFContext', 'creator': 'Microsoft® PowerPoint® 2013', 'creationdate': \"D:20170306114530Z00'00'\", 'moddate': '2018-11-17T11:13:37-06:00', 'title': 'Slide 1', 'author': 'aniketpanda', 'source': '/Users/reejungkim/Documents/Study/Case study/Duke 2017.pdf', 'total_pages': 384, 'page': 19, 'page_label': '20'} page_content='YachtCo\n",
